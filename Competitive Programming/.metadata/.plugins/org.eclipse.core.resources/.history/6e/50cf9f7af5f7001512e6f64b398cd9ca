import java.util.*;
class Q1 
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0)
		{
			int n = sc.nextInt();
			String s = sc.nextLine();
			
			int arr[] = new int[3];
			int max = 0;
			for(int i=0;i<n;i++)
			{
				if(s.charAt(i)=='R')
					arr[0]++;
				else if(s.charAt(i)=='G')
					arr[1]++;
				else
					arr[2]++;
			}
			max = Math.max(arr[0],Math.max(arr[1],arr[2]));
			System.out.println(n-max);
			
		}
	}
}
