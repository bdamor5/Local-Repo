import java.util.*;
class Q1 
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0)
		{
			long n = sc.nextLong();
			long m = sc.nextLong();
			
			if(n%2==1 && m%2==1)
				System.out.println("No");
			else
				System.out.println("Yes");
			
		}
	}
}
